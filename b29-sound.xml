<?xml version="1.0"?>

<PropertyList>

	<fx>

		<!-- Engine 1 -->
		<crank>
			<name>engstart1</name>
			<path>Sounds/engstart.wav</path>
			<property>/engines/engine[0]/cranking</property>
			<volume>
				<offset>0.4</offset>
			</volume>
		</crank>

		<crank>
			<name>crank1</name>
			<mode>looped</mode>
			<path>Sounds/cranking.wav</path>
			<condition>
				<property>/engines/engine[0]/cranking</property>
				<not>
					<property>/engines/engine[0]/running</property>
				</not>
			</condition>
			<volume>
				<offset>0.5</offset>
			</volume>
		</crank>

		<!--<crank>
   <name>cough1</name>
   <path>Sounds/coughing.wav</path>
   <property>/engines/engine[0]/running</property>
   <volume>
    <offset>0.5</offset>
   </volume>
  </crank>-->

		<engine>
			<name>enginecough</name>
			<condition>
				<property>engines/engine/running</property>
			</condition>
			<mode>once</mode>
			<path>Aircraft/dhc2/Sound/startup.wav</path>
			<volume>
				<property>sim/sound/sim-volume</property>
			</volume>
			<position>
				<x>-2.0</x>
				<y>0</y>
				<z>0</z>
			</position>
			<reference-dist>50.0</reference-dist>
			<max-dist>100.0</max-dist>
		</engine>

		<engine>
			<name>engine1_idle</name>
			<mode>looped</mode>
			<path>Aircraft/dc3/Sounds/engine_idle.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<less-than>
					<property>/engines/engine[0]/rpm</property>
					<value>1000</value>
				</less-than>
				<greater-than>
					<property>/engines/engine[0]/rpm</property>
					<value>750</value>
				</greater-than>
			</condition>
			<volume>
				<offset>0.5</offset>
			</volume>
		</engine>

		<engine>
			<name>engine1_running</name>
			<mode>looped</mode>
			<path>Aircraft/dc3/Sounds/engine_running.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<greater-than>
					<property>/engines/engine[0]/rpm</property>
					<value>700</value>
				</greater-than>
			</condition>
			<volume>
				<property>/engines/engine[0]/rpm</property>
				<!--factor>0.00037</factor-->
				<factor>0.00042</factor>
				<max>0.9</max>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<!--factor>0.00037</factor-->
				<factor>0.00020</factor>
				<min>0</min>
				<max>0.8</max>
			</pitch>
		</engine>

