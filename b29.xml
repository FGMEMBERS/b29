	<input>gear/gear-cmd-norm</input>
	<traverse>
	  <setting>
	    <position> 0 </position>
	    <time> 0 </time>
	  </setting>
	  <setting>
	    <position> 1 </position>
	    <time> 5 </time>
	  </setting>
	</traverse>
	<output>gear/gear-pos-norm</output>
      </kinematic>
    </channel> 

