<?xml version="1.0"?>
<PropertyList>

 <path>jrb-wbd-rpm-left.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>3</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>/engines/engine[0]/rpm</property>
  <interpolation>
   <entry>
    <ind>400</ind>
    <dep>205</dep>
   </entry>
   <entry>
    <ind>3500</ind>
    <dep>515</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle2</object-name>
  <property>engines/engine[1]/rpm</property>
  <interpolation>
   <entry>
    <ind>400</ind>
    <dep>205</dep>
   </entry>
   <entry>
    <ind>3500</ind>
    <dep>515</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


